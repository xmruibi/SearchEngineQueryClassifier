<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table-filter match-name="ActionInfo"
		match-catalog="queryTopicClassifer">
	</table-filter>
	<table-filter match-name="QueryInfo"
		match-catalog="queryTopicClassifer">
	</table-filter>
	<table-filter match-name="SubtopicInfo"
		match-catalog="queryTopicClassifer">
	</table-filter>
	<table-filter match-name="TaskInfo"
		match-catalog="queryTopicClassifer">
	</table-filter>
	<table-filter match-name="UserInfo"
		match-catalog="queryTopicClassifer">
	</table-filter>
	<table-filter match-name="UserTopicInfo"
		match-catalog="queryTopicClassifer">
	</table-filter>
</hibernate-reverse-engineering>